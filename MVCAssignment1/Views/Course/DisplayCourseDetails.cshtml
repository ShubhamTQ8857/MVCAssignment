@{
}
@{
    ViewData["Title"] = "DisplayCourseDetails";
}

<h1>Display Course Details</h1>

<table class="table">
    <tr>
        <td>Name</td>
        <td>
            @ViewBag.Name
        </td>
    </tr>

    <tr>
        <td>Gender</td>
        <td>
            @ViewBag.Gender
        </td>
    </tr>


    <tr>
        <td>Course Option</td>
        <td>
            @ViewBag.Options
        </td>
    </tr>
    </tr>
    <tr>
        <td>Selected Course Details</td>
        <td>
            @foreach (string s in ViewBag.course)
            {
                <p>@s</p>
            }
        </td>
    </tr>


</table>